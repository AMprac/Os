//Write a c++ program to print cube of numbers in the given format :
1
1 8
1 8 27

#include <iostream>
using namespace std;
int main() {
// Outer loop for each row
for (int i = 1; i <= 3; i++) {
// Inner loop for printing cubes of numbers in each row
for (int j = 1; j <= i; j++) {
cout << j * j * j << " "; // Printing the cube of j
}
cout << endl; // Move to the next line after each row
}
return 0;
}
